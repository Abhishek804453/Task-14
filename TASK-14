<script> 

function filterNegativeNumbers(numbers) {
  return numbers.filter(function(number) {
    return number >= 0;
  });
}

let numbers=prompt("Enter the number");
const num= filterNegativeNumbers(numbers);
console.log(num);
 
 
 
 
 function diffFrom13(num) {
 if (num <= 13) {
    return 13 - num;
  } else {
    return (num - 13) * 2;
  }
}

let num= prompt("Enter a number");
const diff = diffFrom13(num);

 console.log(diff);
 
 
function fibonacci(num) {
  let fibSeries = [];
  fibSeries[0] = 0;
  fibSeries[1] = 1;

  for (let i = 2; i < num; i++) {
    fibSeries[i] = fibSeries[i - 1] + fibSeries[i - 2];
  }

  return fibSeries;
}

const num= prompt("Enter the number of terms in the Fibonacci series :");

const fibSeries = fibonacci(num);

console.log("Fibonacci series upto" +num+ "terms:" +fibSeries); 
 
 
 
 
function sumMultiples() {
  let sum = 0;
  for (let i = 1; i < 1000; i++) {
    if (i % 3 === 0 || i % 5 === 0) {
      sum += i;
    }
  }
  return sum;
}

console.log(sumMultiples()); 

 
 
 const cube = function(num) {
  return num ** 3;
};

let num= prompt("Enter a number");
console.log(cube(num));
 
 
</script>


